import os, sys
import time

from displayserver import DisplayServer
custom_adb_path = "adb.exe"
my_ds = DisplayServer(custom_adb_path=custom_adb_path)

print my_ds.is_ready()
my_ds.connect()
my_ds.screen_on()
print ("launch")
my_ds.launch_displayserver()

print("display register image")
my_ds.display_image("sdc_register_33_180_0.png")

print("display sdc green 33 35 0")
my_ds.display_image("sdc_green_33_35_0.png")

print("display sdc red 33 44 0")
my_ds.display_image("auo_green_33_44_0.png")

print("display sdc blue 33 44 0")
my_ds.display_image("sdc_blue_33_44_0.png")


my_ds.screen_off()
